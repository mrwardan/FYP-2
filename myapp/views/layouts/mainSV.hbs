      font-family: "Poppins", sans-serif;
      color: white;
    }

    #SlideBarSv-header .profile h1 a,
    #SlideBarSv-header .profile h1 a:hover {
      text-decoration: none;
    }

    #SlideBarSv-header .profile .social-links a {
      font-size: 18px;
      display: inline-block;
      background: #212431;
      color: #fff;
      line-height: 1;
      padding: 8px 0;
      margin-right: 4px;
      border-radius: 50%;
      text-align: center;
      width: 36px;
      height: 36px;
      transition: 0.3s;
    }



