    }

    .pagetitle {
      color: #262626;
      font-family: "Noto Serif", serif;
      font-size: 1.875rem;
      font-weight: 400;

      margin: 3rem;
      margin-left: 15%;
      /* Same as the width of the sidenav */
      font-size: 35px;
      /* Increased text to enable scrolling */
      padding: 0px 10px;



    }

    #edit_inf {
      background-color: #7b857c;
